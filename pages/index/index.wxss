/**index.wxss**/
@import "../../lib/weui.wxss";

.page-section{
  margin-bottom: 20rpx;
}
.page__desc {
    margin-top: 5px;
    color: #888888;
    text-align: center;
    font-size: 14px;
}
page{
  height:100%;
background-color:#f8f8f8;
}
picker{
 line-height:41.41px;
}
.page__hd {
    padding: 40px;
}
.page__title {
    text-align: center;
    font-size: 20px;
    font-weight: 400;
}
.page__bd_spacing {
padding-left:15px;
padding-right:15px;
}
.page__bd {
padding-bottom:40px;
}
.placeholder{
    margin: 5px;
    padding: 0 10px;
    text-align: center;
    background-color: #EBEBEB;
    height: 2.3em;
    line-height: 2.3em;
    color: #cfcfcf;
}
.weui-flex{
  background-color: white;
  margin:20px 0;
  text-align: center;
}
.weui-select{
  height: auto;
}
.icon-exchange{
  background-image:url("data:image/svg+xml;base64,");
